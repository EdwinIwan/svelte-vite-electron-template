    "electron": "^12.0.0",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-releases": "^3.626.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "nodemon": "^2.0.7",
